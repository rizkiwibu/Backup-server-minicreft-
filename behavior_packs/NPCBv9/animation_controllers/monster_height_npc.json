{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.monster_height" : {
			"states" : {
				"default" : {
					"transitions" : [
						{"spider" : "q.variant == 0"},
						{"creeper": "q.variant == 1"},
						{"gaurdian": "q.variant == 2"},
						{"hoglin": "q.variant == 3"},
						{"husk": "q.variant == 4"},
						{"phantom": "q.variant == 5"},
						{"piglin": "q.variant == 6 || q.variant == 7"},
						{"ravager": "q.variant == 8"},
						{"shulker": "q.variant == 9"},
						{"silverfish": "q.variant == 10"},
						{"vex": "q.variant == 15"},
						{"enderman": "q.variant == 27"},
						{"endermite": "q.variant == 28"},
						{"warden": "q.variant == 30"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"spider" : {
					"transitions" : [
						{"default" : "q.variant != 0"}
					],
					"on_entry":[
						"@s small"
					]
				},
				"creeper" : {
					"transitions" : [
						{"default" : "q.variant != 1"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"gaurdian" : {
					"transitions" : [
						{"default" : "q.variant != 2"}
					],
					"on_entry":[
						"@s small"
					]
				},
				"hoglin" : {
					"transitions" : [
						{"default" : "q.variant != 3"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"hoglin" : {
					"transitions" : [
						{"default" : "q.variant != 3"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"husk" : {
					"transitions" : [
						{"default" : "q.variant != 4"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"phantom" : {
					"transitions" : [
						{"default" : "q.variant != 5"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"piglin" : {
					"transitions" : [
						{"default" : "q.variant != 6 && q.variant != 7"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"ravager" : {
					"transitions" : [
						{"default" : "q.variant != 8"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"shulker" : {
					"transitions" : [
						{"default" : "q.variant != 9"}
					],
					"on_entry":[
						"@s small"
					]
				},
				"silverfish" : {
					"transitions" : [
						{"default" : "q.variant != 10"}
					],
					"on_entry":[
						"@s xtra_small"
					]
				},
				"vex" : {
					"transitions" : [
						{"default" : "q.variant != 15"}
					],
					"on_entry":[
						"@s xtra_small"
					]
				},
				"enderman" : {
					"transitions" : [
						{"default" : "q.variant != 27"}
					],
					"on_entry":[
						"@s tall"
					]
				},
				"endermite" : {
					"transitions" : [
						{"default" : "q.variant != 28"}
					],
					"on_entry":[
						"@s xtra_small"
					]
				},
				"warden" : {
					"transitions" : [
						{"default" : "q.variant != 30"}
					],
					"on_entry":[
						"@s tall"
					]
				}
			}
		}
	}
}
