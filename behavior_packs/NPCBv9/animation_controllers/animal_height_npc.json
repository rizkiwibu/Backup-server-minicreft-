{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.animals_height" : {
			"states" : {
				"default" : {
					"transitions" : [
						{"horse" : "q.variant == 3"},
						{"donkey" : "q.variant == 7"},
						{"llama" : "q.variant == 10"},
						{"strider" : "q.variant == 17"},
						{"reindeer" : "q.variant == 25"},
						{"sniffer" : "q.variant == 32"}
					],
					"on_entry":[
						"@s small"
					]
				},
				"horse" : {
					"transitions" : [
						{"default" : "q.variant != 3"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"donkey" : {
					"transitions" : [
						{"default" : "q.variant != 7"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"llama" : {
					"transitions" : [
						{"default" : "q.variant != 10"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"strider" : {
					"transitions" : [
						{"default" : "q.variant != 17"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"reindeer" : {
					"transitions" : [
						{"default" : "q.variant != 25"}
					],
					"on_entry":[
						"@s medium"
					]
				},
				"sniffer" : {
					"transitions" : [
						{"default" : "q.variant != 32"}
					],
					"on_entry":[
						"@s medium"
					]
				}
			}
		}
	}
}
